pkg test;

ext void printf(str fmt, var str args);

struct Person
{
    Name name;
    int  age;
}

struct Name
{
    str first_name;
    str last_name;
}

def int main(str[] args)
{
    var Person p = test();

    printf("%s\n", p.name.first_name);
    printf("%s\n", p.name.last_name);
    printf("%d\n", p.age);
    return 0;
}

def Person test()
{
    return { { "Kiana", "Bennett" }, 22};
}
