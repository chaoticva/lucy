pkg test;

ext void printf(str fmt, var str args);

struct Test {}

def int main()
{
    var Test t;

    printn(69.69);
    printn("Hello");
    printn(69);
    printn(t);

    return 0;
}

macro void printn(x)
{
    switch (x.$typekind)
    {
        case "STRUCT": printf("%s {}\n", x.$typename);
        case "BUILTIN":
            switch (x.$typeid)
            {
                case str.$typeid:    printf("%s\n", x);
                case int.$typeid:    printf("%d\n", x);
                case double.$typeid: printf("%.2f\n", x);
            }
    }
}
